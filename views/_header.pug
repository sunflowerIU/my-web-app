//- navbar
header
    if title==='Real-Estate'
        .bg
            nav.navbar.navbar-expand-md.navbar-dark.fixed-top.bg-dark
                .container-fluid
                    a.navbar-brand(href=`/`) Real-estate
                    button.navbar-toggler(type='button', data-bs-toggle='collapse', data-bs-target='#navbarCollapse', aria-controls='navbarCollapse', aria-expanded='false', aria-label='Toggle navigation')
                        span.navbar-toggler-icon
                    #navbarCollapse.collapse.navbar-collapse.justify-content-end
                        ul.navbar-nav.justify-content-end
                            li.nav-item
                                a.nav-link(href='/contact-us') Contact us
                                // if you have account
                            if user
                                li.nav-item
                                    a.nav-link(href='/create-property') Create Property
                                li.nav-item.dropdown
                                    a#navbarDropdownMenuLink.nav-link.dropdown-toggle(href='#', role='button', data-bs-toggle='dropdown', aria-expanded='false') Username
                                    ul.dropdown-menu(aria-labelledby='navbarDropdownMenuLink')
                                        li
                                            a.dropdown-item(href='/my-account') profile
                                        li
                                            a.dropdown-item.logout-btn(href='') Log out
                                // if you dont have account
                            else
                                li.nav-item
                                    button.btn.btn-outline-primary.loginBtn(type='button') Login
                                li.nav-item
                                    button.btn.btn-primary.signupBtn(type='button') Sign up

                            //- +checkUser(user)
    else
        nav.navbar.navbar-expand-md.navbar-dark.fixed-top.bg-dark
            .container-fluid
                a.navbar-brand(href=`/`) Real-estate
                button.navbar-toggler(type='button', data-bs-toggle='collapse', data-bs-target='#navbarCollapse', aria-controls='navbarCollapse', aria-expanded='false', aria-label='Toggle navigation')
                    span.navbar-toggler-icon
                #navbarCollapse.collapse.navbar-collapse.justify-content-end
                    ul.navbar-nav.justify-content-end
                        li.nav-item
                            a.nav-link(href='/contact-us') Contact us
                                // if you have account
                            if user
                                li.nav-item
                                    a.nav-link(href='/create-property') Create Property
                                li.nav-item.dropdown
                                    a#navbarDropdownMenuLink.nav-link.dropdown-toggle(href='#', role='button', data-bs-toggle='dropdown', aria-expanded='false') Username
                                    ul.dropdown-menu(aria-labelledby='navbarDropdownMenuLink')
                                        li
                                            a.dropdown-item(href='/my-account') profile
                                        li
                                            a.dropdown-item.logout-btn(href='#') Log out
                                // if you dont have account
                            else
                                li.nav-item
                                    button.btn.btn-outline-primary.loginBtn(type='button') Login
                                li.nav-item
                                    button.btn.btn-primary.signupBtn(type='button') Sign up



